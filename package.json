{
  "name": "uell-ionic-mobile",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "start:dev": "ng serve -o --configuration development",
    "start:qa": "ng serve -o --configuration qa",
    "start:stage": "ng serve -o --configuration stage",
    "start:prod": "ng serve --configuration production",
    "build": "ionic build",
    "build:dev": "ionic build --configuration development",
    "build:qa": "ionic build --configuration qa",
    "build:stage": "ionic build --configuration stage",
    "build:prod": "ionic build --configuration production",
    "sync": "npx cap sync",
    "sync:android": "npx cap sync android",
    "sync:ios": "npx cap sync ios",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "build:android:dev": "npm run prepare:firebase:dev && npm run build:dev && npm run sync:android && npm run open:android",
    "build:android:qa": "npm run prepare:firebase:dev && npm run build:qa && npm run sync:android && npm run open:android",
    "build:android:stage": "npm run prepare:firebase:dev && npm run build:stage && npm run sync:android && npm run open:android",
    "build:android:prod": "npm run prepare:firebase:prod && npm run build:prod && npm run sync:android && npm run open:android",
    "build:ios:dev": "npm run prepare:firebase:dev && npm run build:dev && npm run sync:ios && npm run open:ios",
    "build:ios:qa": "npm run prepare:firebase:dev && npm run build:qa && npm run sync:ios && npm run open:ios",
    "build:ios:stage": "npm run prepare:firebase:dev && npm run build:stage && npm run sync:ios && npm run open:ios",
    "build:ios:prod": "npm run prepare:firebase:prod && npm run build:prod && npm run sync:ios && npm run open:ios",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "copy:ios": "ionic build && npx cap copy ios",
    "android:stage": "ionic build --configuration stage && npx cap sync android",
    "ios:stage": "ionic build --configuration stage && npx cap sync ios",
    "prepare:firebase:dev": "node scripts/prepare-firebase.js dev",
    "prepare:firebase:prod": "node scripts/prepare-firebase.js prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@capacitor/android": "^7.0.1",
    "@capacitor/app": "7.0.0",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/camera": "^7.0.0",
    "@capacitor/core": "^7.0.1",
    "@capacitor/device": "^7.0.0",
    "@capacitor/filesystem": "^7.0.0",
    "@capacitor/haptics": "7.0.0",
    "@capacitor/ios": "^7.0.1",
    "@capacitor/keyboard": "7.0.0",
    "@capacitor/push-notifications": "^7.0.0",
    "@capacitor/splash-screen": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "@ionic/angular": "^7.8.6",
    "@ionic/pwa-elements": "^3.3.0",
    "@ionic/storage-angular": "^4.0.0",
    "amazon-cognito-identity-js": "^6.3.12",
    "ionicons": "^7.0.0",
    "moment": "^2.30.1",
    "ngx-countup": "^13.2.0",
    "rxjs": "^7.5.0",
    "swiper": "^11.2.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.0",
    "@angular-eslint/builder": "^18.0.0",
    "@angular-eslint/eslint-plugin": "^18.0.0",
    "@angular-eslint/eslint-plugin-template": "^18.0.0",
    "@angular-eslint/schematics": "^18.0.0",
    "@angular-eslint/template-parser": "^18.0.0",
    "@angular/cli": "^18.0.0",
    "@angular/compiler-cli": "^18.0.0",
    "@angular/language-service": "^18.0.0",
    "@capacitor/cli": "^7.0.1",
    "@ionic/angular-toolkit": "12.1.1",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.0"
  },
  "description": "An Ionic project"
}
